{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "./dist",
    "paths": {
      "css-modules-loader-core": [
        "./typings/css-modules-loader-core/index.d.ts"
      ],
      "reserved-words": ["./typings/reserved-words/index.d.ts"]
    }
  }
}
