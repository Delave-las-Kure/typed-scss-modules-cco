  ],
  "bin": "./dist/cli.js",
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.0",
    "@types/node-sass": "^3.10.32",
    "@types/param-case": "^1.1.2",
    "@types/yargs": "^12.0.8",
    "camelcase": "^5.0.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.11.0",
    "param-case": "^2.1.1",
    "prettier": "^1.16.4",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "peerDependencies": {
    "node-sass": "^4.11.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "css-modules-loader-core": "^1.1.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.3",
    "path": "^0.12.7",
    "yargs": "^12.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,scss,tsx,ts}": [
      "prettier --write",
      "git add"
    ]
